  "homepage": "https://github.com/erikakers/express-spa#readme",
  "dependencies": {
    "autoprefixer": "6.4.1",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-1": "6.13.0",
    "css-loader": "0.25.0",
    "dotenv": "2.0.0",
    "eslint": "3.5.0",
